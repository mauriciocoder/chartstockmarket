function handleWsMessageReturn(message) {
	if (message.length != stocks.length) {
		stocks = message;
        updateGraph();
	} else {
		for (var i = 0; i < stocks.length; i++) {
			var itemFound = false;
			for (var j = 0; j < message.length; j++) {
				if (message[j] == stocks[i]) {
					itemFound = true;
				}
			}
			if (!itemFound) {
				stocks = message;
				updateGraph();
                break;
			}
		}
	}
}

//var stocks = ["AAPL", "IBM"];
