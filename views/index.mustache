{{^stocks}}var stocks = [];{{/stocks}}
var stocksLoaded = 0;
var graphData = [];
var ws = new WebSocket("ws://fcc-fullstack-working-projects-mauriciobonetti.c9users.io/", "echo-protocol");

ws.onmessage = function (event) {
	var serverStocks = event.data.split(",");
	stocks = serverStocks;
	updateGraph();
	buildCRUDTable();
};

$(document).ready(function() {
	$("#btnAdd").unbind("click").click(addStock);
	google.charts.load("current", {packages: ["corechart", "line"]});
	//google.charts.setOnLoadCallback(drawChart);
	// Obter as stocks do socket
	// Renderizar a tabela
	buildCRUDTable();
	//alert("graphData = " + graphData);
	updateGraph();
});

function addStock() {
	var stock = $("select[name='selectionStock'] option:selected").text();
	stocks.push(stock);
	updateGraph();
	buildCRUDTable();
	ws.send(stocks);    // Atualiza websocket
}

function buildCRUDTable() {
	$(".rowStock").remove();
	for (var i = 0; i < stocks.length; i++) {
		var url = 'https://query.yahooapis.com/v1/public/yql?q=select symbol, Name from yahoo.finance.quotes where symbol in ("' + stocks[i] + '")&format=json&diagnostics=true&env=store://datatables.org/alltableswithkeys&callback=';
		$.get(url, handleCRUDData);
	}
}

function handleCRUDData(data) {
	var name = data.query.results.quote.Name;
	var symbol = data.query.results.quote.symbol;
	$("#CRUDtable").append("<tr id='" + symbol + "' class='rowStock'><td>" + symbol + "</td><td>" + name + "</td><td><button class='btnRemove' data-symbol='" + symbol + "'>Remove</button></td></tr>");
	$(".btnRemove").unbind("click").click(removeStock);
}

function removeStock() {
	var symbol = $(this).data("symbol");
	$("#" + symbol).remove();	// Remove row from CRUDTable
	// Remove da lista
	removeFromList(symbol);
	ws.send(stocks);	// Atualiza websocket
	// Redesenha o gráfico
	updateGraph();
}

function updateGraph() {
	graphData = [];
	stocksLoaded = 0;
	if (stocks.length > 0) {
		$("#chart_div").html("<h3>Loading Graph...</h3>");
		graphData[0] = ["DATE"].concat(stocks);
		for (var i = 0; i < stocks.length; i++) {
			var url = loadURL(stocks[i]);
			$.get(url, handleStockData);
		}
	} else {
		$("#chart_div").html("");
	}
}

function removeFromList(symbol) {
	var i = findSymbolIndex(symbol);
	stocks.splice(i, 1);
} 

function handleStockData(data) {
	var histData = data.query.results.quote;
	var synbolIndex = findSymbolIndex(histData[0].Symbol) + 1;
	for (var i = 0; i < histData.length; i++) {
		var date = histData[i].Date;
		var close = histData[i].Close;
		if (!graphData[i + 1]) {
			graphData[i + 1] = [];
		}
		graphData[i + 1][0] = new Date(date);
		graphData[i + 1][synbolIndex] = parseFloat(close);
	}
	stocksLoaded++;
	if (stocksLoaded == stocks.length) {
		drawChart();
	}
}

function findSymbolIndex(symbol) {
	if (stocks) {
		for (var i = 0; i < stocks.length; i++) {
			if (stocks[i] == symbol) {
				return i;
			}
		}
	}
}

function loadURL(stock) {
	var prefix = "https://query.yahooapis.com/v1/public/yql?q=";
	
	var today = new Date();
	var todayString = today.toISOString().slice(0,10); // YYY-MM-DD
	
	var lastYear = today;
	lastYear.setFullYear(today.getFullYear() - 1);
	var lastYearString = lastYear.toISOString().slice(0,10); // YYY-MM-DD
	
	var query = "select * from yahoo.finance.historicaldata where symbol = '" + stock + "' and startDate = '" + lastYearString + "' and endDate = '" + todayString + "'";
	var params = "&format=json&diagnostics=true&env=store://datatables.org/alltableswithkeys&callback=";
	return prefix + query + params;
}

function drawChart() {
	//alert("graphData = " + JSON.stringify(graphData));
	var data = google.visualization.arrayToDataTable(graphData);
	var options = {
		hAxis: {
		  title: "Date"
		},
		vAxis: {
		  title: "Value"
		}
	};
	var chart = new google.visualization.LineChart(document.getElementById("chart_div"));
	$("#chart_div").html("");
	chart.draw(data, options);
}
</script>	
